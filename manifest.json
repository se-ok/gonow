{
  "manifest_version": 3,
  "name": "GoNow",
  "version": "0.1.0",
  "description": "Fast, keyboard-driven navigation to bookmarked URLs via the omnibox.",
  "permissions": ["storage", "search"],
  "omnibox": {
    "keyword": "`"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
